{
    "name": "repairq/php-azure-sdk",
    "description": "This project provides a set of PHP client libraries that make it easy to access Windows Azure tables, blobs, queues, service runtime and service management APIs.",
    "keywords": [ "php", "azure", "sdk" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lucas PeÃ±a",
            "email": "lucas.pena@servicecentral.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "microsoft/azure-storage-common": "1.5.2",
        "guzzlehttp/guzzle": "7.9",
        "laminas/laminas-mime": "^2.12"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^4.8.35",
        "phpdocumentor/phpdocumentor": "^2.8",
        "sebastian/phpcpd": "^2.0",
        "phploc/phploc": "^2.1",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6",
        "mayflower/php-codebrowser": "^1.1",
        "friendsofphp/php-cs-fixer": "^1.12.2"
    },
    "autoload": {
        "psr-4": { "WindowsAzure\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
